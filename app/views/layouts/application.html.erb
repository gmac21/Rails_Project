<!DOCTYPE html>
<html>
<head>
  <title>Cinema</title>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  
</head>
<nav class="navbar navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="#">King City Cinemas</a>
    </div>
    <ul class="nav navbar-nav">
     
      <li><a href="http://localhost:3000/">Home</a></li>
      <li><a href="http://localhost:3000/movies">Movie</a></li> 
      <li><a href="http://localhost:3000/users">User</a></li> 
	  <li><a href="http://localhost:3000/bookings">Booking</a></li>
      <li><a href="http://localhost:3000/screenings">Screening</a></li> 
      <li><a href="http://localhost:3000/theatres">Theatre</a></li>
	  <li><a href="https://github.com/gmac21">Github = Gmac21</a></li>
	  
    </ul>
	
  </div>
  
</nav> 




<% flash.each do |key, value| %>
     <div class = "flash<%= key %>">
	   <%= value %>
	   
	   </div>
	   <% end %>
<ul>
<% if !logged_in? %>
		<li><%= link_to "Sign In", login_path %></li>
		<% else %>
		<li>Signed in as <%= @current_user.name %></li>
		<li><%= link_to "Sign Out", logout_path %></li>
		<%end %>
	</ul>
</div>



</body>
</html>


<%= yield %>

</body>
</html>
